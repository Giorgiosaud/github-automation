{
  "name": "github-automation",
  "version": "5.1.14",
  "description": "A github automation CLI for repetitive works",
  "type": "module",
  "author": "Jorge Saud @Giorgiosaud",
  "bin": {
    "github-automation": "bin/run"
  },
  "homepage": "https://github.com/Giorgiosaud/github-automation",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giorgiosaud/github-automation.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^4.5.4",
    "@oclif/plugin-help": "^6.2.33",
    "@oclif/plugin-plugins": "^5.4.48",
    "@octokit/oauth-app": "^8.0.2",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "inquirer": "^12.9.6",
    "js-nacl": "^1.4.0",
    "js-yaml": "^4.1.0",
    "octokit": "^5.0.3",
    "pino": "^9.13.0",
    "tweetnacl-ts": "^1.0.3"
  },
  "devDependencies": {
    "@jest/types": "^30.2.0",
    "@oclif/test": "^4.1.14",
    "@octokit/types": "^15.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-nacl": "^1.3.4",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.36.0",
    "eslint-config-oclif": "^6.0.106",
    "eslint-plugin-unicorn": "^61.0.2",
    "jest": "^30.2.0",
    "jest-config": "^30.2.0",
    "jest-worker": "^30.2.0",
    "oclif": "^4.22.27",
    "pino-pretty": "^13.1.1",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "oclif": {
    "bin": "github-automation",
    "dirname": "github-automation",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "branch-protection-rules": {
        "description": "Copy protection rules from a repo to many others"
      },
      "collaborators": {
        "description": "Manage collaborators in repository"
      },
      "create-environment": {
        "description": "Create environment in repository"
      },
      "delete-secret": {
        "description": "Delete secret in repository"
      },
      "list-org-repositories": {
        "description": "List all repositories in organization"
      },
      "repos-with-secret": {
        "description": "List all repositories with secret"
      },
      "set-secret": {
        "description": "Set secret in repository with env or not"
      },
      "set-vars": {
        "description": "Set vars in repository with env or not"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/Giorgiosaud/github-automation/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
